{
  "name": "DoraDocumentApproval",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap-cloud-sdk/connectivity": "^4.1.2",
    "@sap-cloud-sdk/http-client": "^4.1.2",
    "@sap-cloud-sdk/openapi-generator": "^4.1.2",
    "@sap-cloud-sdk/resilience": "^4.1.2",
    "@sap/cds": "^9",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "private": true,
  "cds": {
    "requires": {
      "[production]": {
        "auth": "xsuaa"
      },
      "Ariba": {
        "kind": "odata",
        "credentials": {
          "destination": "AribaRequisitionCustomViewDora"
        }
      },
      "destinations": {
          "kind": "rest",
          "binding": {
            "type": "cf",
            "instance": "ariba-dest",
            "key": "ariba-dest-key"
          }
        }
    }
  }
}
